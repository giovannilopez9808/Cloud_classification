python Test_neural_models.py ratio RS "Bidirectional LSTM" Suroeste


python Test_neural_models.py ratio GHI "Bidirectional LSTM" Noroeste
python Test_neural_models.py ratio GHI "Bidirectional LSTM" Noreste
python Test_neural_models.py ratio GHI "Bidirectional LSTM" Sureste2
python Test_neural_models.py ratio GHI "Bidirectional LSTM" Suroeste


