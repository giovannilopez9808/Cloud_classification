python Test_neural_models.py diff GHI "Bidirectional LSTM" Noreste
python Test_neural_models.py diff GHI "Bidirectional LSTM" Sureste2
python Test_neural_models.py diff GHI "Bidirectional LSTM" Suroeste

python Test_neural_models.py diff RS "Bidirectional LSTM" Noroeste
python Test_neural_models.py diff RS "Bidirectional LSTM" Noreste
python Test_neural_models.py diff RS "Bidirectional LSTM" Sureste2
python Test_neural_models.py diff RS "Bidirectional LSTM" Suroeste
