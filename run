date
python Train_neural_models.py ratio GHI "Attention CNN" Noreste
python Train_neural_models.py ratio GHI "Attention CNN" Noroeste
python Train_neural_models.py ratio GHI "Attention CNN" Sureste2
python Train_neural_models.py ratio GHI "Attention CNN" Suroeste
python Train_neural_models.py diff RS "Attention CNN" Noreste
python Train_neural_models.py diff RS "Attention CNN" Noroeste
python Train_neural_models.py diff RS "Attention CNN" Sureste2
python Train_neural_models.py diff RS "Attention CNN" Suroeste
python Train_neural_models.py diff GHI "Attention CNN" Noreste
python Train_neural_models.py diff GHI "Attention CNN" Noroeste
python Train_neural_models.py diff GHI "Attention CNN" Sureste2
python Train_neural_models.py diff GHI "Attention CNN" Suroeste
date
