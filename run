#!/bin/bash
#SBATCH --partition=GPU
#SBATCH --job-name=Class
#SBATCH --output=run.log
#SBATCH --mail-type=END,FAIL
#SBATCH --mail-user=giovanni.lopez@cimat.mx
cd /home/est_posgrado_giovanni.lopez/Cloud_classification/Scripts
source /opt/anaconda3_titan/bin/activate
conda activate tensorflow
export TF_ENABLE_ONEDNN_OPTS=0
hostname
date
echo "--------------------------------------------"
echo "Diff RS"
python neural_models.py diff RS perceptron
echo "--------------------------------------------"
echo "Diff GHI"
python neural_models.py diff GHI perceptron
echo "--------------------------------------------"
echo "ratio RS"
python neural_models.py ratio RS perceptron
echo "--------------------------------------------"
echo "ratio GHI"
python neural_models.py ratio GHI perceptron
date
